{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "/popup/popup.html",
        "default_icon": {
          "16": "images/icon-16.png",
          "32": "images/icon-32.png",
          "48": "images/icon-48.png",
          "128": "images/icon-128.png"
        }
    },
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "host_permissions": [
      "https://web.whatsapp.com/*",
      "https://agentes.casino365online.net/*"
    ],
    "permissions": [
      "tabGroups", "scripting", "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "commands": {
        "copyUser":{
          "suggested_key": {
            "default": "Ctrl+Comma",
            "mac": "Command+Comma"
          },
          "description": "Copia el usuario de whatsapp"
      },
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+B",
            "mac": "Command+B"
          }
        }
      }
}

  